type Query @extends{
    products(titleFilter: String): [Product]
}

type Product @key(fields: "id") @key(fields: "sellerId") {
    id: Int!
    title: String!
    sellerId: Int!
}

type Seller @key(fields: "id") @extends {
    id: Int! @external
    productsOf: [Product!]
    productsOfBatchDataLoader: [Product!]
}
